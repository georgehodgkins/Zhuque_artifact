.PHONY: sgl
sgl : 
	make -C ./genome -f Makefile.sgl
	make -C ./yada -f Makefile.sgl
	make -C ./kmeans -f Makefile.sgl
	make -C ./vacation -f Makefile.sgl
	make -C ./ssca2 -f Makefile.sgl
	make -C ./bayes -f Makefile.sgl
	make -C ./intruder -f Makefile.sgl
	make -C ./labyrinth -f Makefile.sgl

.PHONY: seq
seq : 
	make -C ./genome -f Makefile.seq
	make -C ./yada -f Makefile.seq
	make -C ./kmeans -f Makefile.seq
	make -C ./vacation -f Makefile.seq
	make -C ./ssca2 -f Makefile.seq
	make -C ./bayes -f Makefile.seq
	make -C ./intruder -f Makefile.seq
	make -C ./labyrinth -f Makefile.seq
	
.PHONY: clean
clean : 
	make -C ./genome -f Makefile.sgl clean
	make -C ./yada -f Makefile.sgl clean
	make -C ./kmeans -f Makefile.sgl clean
	make -C ./vacation -f Makefile.sgl clean
	make -C ./ssca2 -f Makefile.sgl clean
	make -C ./bayes -f Makefile.sgl clean
	make -C ./intruder -f Makefile.sgl clean
	make -C ./labyrinth -f Makefile.sgl clean